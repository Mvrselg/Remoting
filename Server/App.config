<configuration>
	<system.runtime.remoting>
		<application>

			<channels>
				<channel 
				secure="true"
				protectionLevel="EncryptAndSign"
				impersonate= "false">
					<serverProviders>
						<formatter ref="soap" typeFilterLevel="Full"/>
					</serverProviders>
					<clientProviders>
						<formatter ref="soap"/>
					</clientProviders>
				</channel>
			</channels>
			<service>
				<activated type="ClassLibrary1.Class1, ClassLib"/>
			</service>
		</application>
	</system.runtime.remoting>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
	</startup>
</configuration>
